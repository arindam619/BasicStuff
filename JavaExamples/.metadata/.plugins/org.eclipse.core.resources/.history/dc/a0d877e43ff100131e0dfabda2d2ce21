package basic;

public class StringExample {
	
	
	public void runStrExample() {
		
		
		//String greet = new String("Hello world...");
		String greet = "hello";
		
		if ( greet == "hello" ) {
			System.out.println("hello is present");
		}
		else
			System.out.println("hello is not present");
		
		
		//System.out.println(greet);
		
	}
	
	
}
